{% if isFile == false %}


{% for fichier_arr in liste_fichier %}

	<div draggable='true' class="elem">
	<img draggable='false' class="fichier icon" data-path="{{fichier_arr}}" src="./img/icones/document.svg" alt="icone du dossier">
	<p class="fichier pr-2 pl-2" data-path="{{fichier_arr}}">{{fichier_arr}}</p>
	</div>

{% endfor %}

{% else %}

	<div class="elem">{{file}}</div>

{% endif %}